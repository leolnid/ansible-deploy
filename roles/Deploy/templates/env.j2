APP_NAME={{ application_name }}
APP_ENV={{ application_env }}
APP_KEY=
APP_DEBUG={{ application_debug }}
APP_URL={{ application_url }}

LOG_CHANNEL=stack
SENTRY_LARAVEL_DSN={{ sentry_laravel_dns }}
SENTRY_TRACES_SAMPLE_RATE={{ sentry_traces_sample_rate }}
SENTRY_PROFILES_SAMPLE_RATE={{ sentry_profiles_sample_rate }}

DB_CONNECTION=mysql
DB_HOST={{ mysql_host }}
DB_PORT={{ mysql_port }}
